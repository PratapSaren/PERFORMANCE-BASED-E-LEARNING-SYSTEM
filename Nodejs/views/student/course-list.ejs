<!doctype html>
<html lang="en" class="no-js">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1, maximum-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <meta name="theme-color" content="#3e454c">

    <title>Edusys || Admin-Login</title>

    <!-- Font awesome -->
    <link rel="stylesheet" href="/public/admin-css/css/font-awesome.min.css">
    <!-- Sandstone Bootstrap CSS -->
    <link rel="stylesheet" href="/public/admin-css/css/bootstrap.min.css">
    <!-- Bootstrap Datatables -->
    <link rel="stylesheet" href="/public/admin-css/css/dataTables.bootstrap.min.css">
    <!-- Bootstrap social button library -->
    <link rel="stylesheet" href="/public/admin-css/css/bootstrap-social.css">
    <!-- Bootstrap select -->
    <link rel="stylesheet" href="/public/admin-css/css/bootstrap-select.css">
    <!-- Bootstrap file input -->
    <link rel="stylesheet" href="/public/admin-css/css/fileinput.min.css">
    <!-- Awesome Bootstrap checkbox -->
    <link rel="stylesheet" href="/public/admin-css/css/awesome-bootstrap-checkbox.css">
    <!-- Admin Stye -->
    <link rel="stylesheet" href="/public/admin-css/css/style.css">


    <link rel="stylesheet" href="/public/student-css/style.css">

    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Cinzel:wght@700&display=swap" rel="stylesheet">

    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Crete+Round&display=swap" rel="stylesheet">



</head>

<body>
    <div class="brand clearfix">
        <a href="#" class="navbar-brand">Edu<b>sys</b></a>
        <span class="menu-btn"><i class="fa fa-bars"></i></span>
        <ul class="ts-profile-nav">

            <li class="ts-account">
                <a href="#"><img src="/public/admin-css/img/ts-avatar.jpg" class="ts-avatar hidden-side" alt="">
                    <%= user.username %><i class="fa fa-angle-down hidden-side"></i>
                </a>
                <ul>
                    <li><a href="/profile">My Account</a></li>
                    
                    <li><a href="/logout">Logout</a></li>
                </ul>
            </li>
        </ul>
    </div>

    <div class="ts-main-content" style=" height: 90vh;">
        <nav class="ts-sidebar">
            <ul class="ts-sidebar-menu">

                <li class="ts-label">Main</li>
                <li class="open"><a href="/student/student-dashboard"><i class="fa fa-dashboard"></i> Dashboard</a></li>
                <li><a href="#"><i class="fa fa-desktop"></i> Courses </a>
                    <ul>
                        <li><a href="/student/course-list"> Courses List </a></li>
                        <li><a href="/student/my-course"> My Courses </a></li>

                    </ul>
                </li>
               


                
                

                <!-- Account from above -->
                <ul class="ts-profile-nav">

                    <li class="ts-account">
                        <a href="#"><img src="img/ts-avatar.jpg" class="ts-avatar hidden-side" alt="">
                            <%= user.username %> <i class="fa fa-angle-down hidden-side"></i>
                        </a>
                        <ul>
                            <li><a href="/profile">My Account</a></li>
                            
                            <li><a href="/logout">Logout</a></li>
                        </ul>
                    </li>
                </ul>

            </ul>
        </nav>


        <div class="content-wrapper">
            <div class="container-fluid">

                <div class="row">
                    <div class="col-md-12">

                        <h2 class="page-title">List Of Courses</h2>

                        <div class="row">
                            <div class="col-md-12">
                                <div class="row">



                                    <% obj1.forEach(element=> {%>
                                        <% var arr1=element %>





                                            <div class="col-md-4" style="width: 18rem;">

                                                <div class="panel panel-info">
                                                    <div class="panel-heading">
                                                        <h3 class="panel-title" style="text-align: center;">
                                                            <%= arr1.coursename %>
                                                        </h3>
                                                    </div>
                                                    <div class="panel-body"
                                                        style="background-color: rgb(198, 234, 255);">
                                                        <p style="text-align: center; font-family: 'Cinzel', serif;">
                                                            Level :- <%= arr1.courselevel %>
                                                        </p>
                                                        <div>
                                                            <p
                                                                style="text-align: center; font-family: 'Cinzel', serif;">
                                                                Language :- <%= arr1.language %>
                                                            </p>
                                                        </div>
                                                    </div>
                                                    <div class="panel-footer">
                                                        <div style="text-align: center;">

                                                            <button type="button" class="btn btn-primary"
                                                                data-toggle="modal" data-target="#first">
                                                                Go To Course
                                                            </button>
                                                        </div>
                                                    </div>
                                                </div>


                                            </div>

                                            <%})%>


                            <!--- course - 2--->


                            <% obj2.forEach(element=> {%>
                                <% var arr2=element %>





                                    <div class="col-md-4" style="width: 18rem;">

                                        <div class="panel panel-info">
                                            <div class="panel-heading">
                                                <h3 class="panel-title" style="text-align: center;">
                                                    <%= arr2.coursename %>
                                                </h3>
                                            </div>
                                            <div class="panel-body"
                                                style="background-color: rgb(198, 234, 255);">
                                                <p style="text-align: center; font-family: 'Cinzel', serif;">
                                                    Level :- <%= arr2.courselevel %>
                                                </p>
                                                <div>
                                                    <p
                                                        style="text-align: center; font-family: 'Cinzel', serif;">
                                                        Language :- <%= arr2.language %>
                                                    </p>
                                                </div>
                                            </div>
                                            <div class="panel-footer">
                                                <div style="text-align: center;">

                                                    <button type="button" class="btn btn-primary"
                                                        data-toggle="modal" data-target="#second">
                                                        Go To Course
                                                    </button>
                                                </div>
                                            </div>
                                        </div>


                                    </div>

                                    <%})%>              


                            <!--End-->

                                </div>
                            </div>
                        </div>

                        <!-- Modal For First Course-->
                        <div class="modal fade" id="first" tabindex="-1" role="dialog"
                            aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
                            <div class="modal-dialog modal-dialog-centered" role="document">
                                <div class="modal-content" style="border: 2px solid coral;">
                                    <div class="modal-header" style="background-color: coral;">
                                        <h5 class="modal-title"
                                            style="text-align: center; font-size:larger; font-family: 'Crete Round', serif; "
                                            id="exampleModalCenterTitle">Web Development</h5>
                                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                            <span aria-hidden="true">&times;</span>
                                        </button>
                                    </div>
                                    <div class="modal-body"
                                        style="height: 300px; font-size: large; text-align: center; border: 2px solid rgb(230, 37, 37);">


                                        <form action="/student/course-list" method="POST">


                                            <div class="ruther" style="text-align: center; ">
                                                
                                                <div class="form-check" style=" padding-top: 60px;">
                                                    <input class="form-check-input" type="radio" name="option"
                                                        id="flexRadioDefault2" value="two" >
                                                    <label class="form-check-label" for="flexRadioDefault2">
                                                        Full Course For Free Trial
                                                    </label>
                                                </div>


                                            </div>
                                            <div class="ford" style="margin-top: 50px; text-align: center;">
                                                <input type="hidden" name="item" value="200">
                                                
                                                <button type="submit" class="btn btn-primary">Continue</button>
                                            </div>

                                        </form>

                                    </div>





                                </div>
                            </div>
                        </div>



                        <!-- Modal For Second Course-->
                        <div class="modal fade" id="second" tabindex="-1" role="dialog"
                            aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
                            <div class="modal-dialog modal-dialog-centered" role="document">
                                <div class="modal-content" style="border: 2px solid coral;">
                                    <div class="modal-header" style="background-color: coral;">
                                        <h5 class="modal-title"
                                            style="text-align: center; font-size:larger; font-family: 'Crete Round', serif; "
                                            id="exampleModalCenterTitle">Programming Using Python</h5>
                                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                            <span aria-hidden="true">&times;</span>
                                        </button>
                                    </div>
                                    <div class="modal-body"
                                        style="height: 300px; font-size: large; text-align: center; border: 2px solid rgb(230, 37, 37);">


                                        <form action="/student/course-list" method="POST">


                                            <div class="ruther" style="text-align: center; ">
                                                
                                                <div class="form-check" style=" padding-top: 60px;">
                                                    <input class="form-check-input" type="radio" name="option"
                                                        id="flexRadioDefault2" value="one" >
                                                    <label class="form-check-label" for="flexRadioDefault2">
                                                        Full Course, No Certificate
                                                    </label>
                                                </div>


                                            </div>
                                            <div class="ford" style="margin-top: 50px; text-align: center;">
                                                <input type="hidden" name="item" value="201">
                                                
                                                <button type="submit" class="btn btn-primary">Continue</button>
                                            </div>

                                        </form>

                                    </div>





                                </div>
                            </div>
                        </div>
                  






                        <div class="row">


                        </div>

                        <div class="row">


                        </div>

                    </div>
                </div>

            </div>




        </div>

        <footer style="position: fixed;
	left: 0;
	bottom: 0;
	width: 100%;
	background-color: red;
	color: white;
	text-align:center;
	">
            <p style="padding-bottom: 0px;
		margin-bottom: 0px;">Copyright &copy 2021 <a href="#"
                    style="color:rgb(192, 150, 247);"><strong>Edusys.com</strong></a> | All Rights Reserved</p>
        </footer>

        <!-- Loading Scripts -->
        <script src="/public/admin-css/js/jquery.min.js"></script>
        <script src="/public/admin-css/js/bootstrap-select.min.js"></script>
        <script src="/public/admin-css/js/bootstrap.min.js"></script>
        <script src="/public/admin-css/js/jquery.dataTables.min.js"></script>
        <script src="/public/admin-css/js/dataTables.bootstrap.min.js"></script>
        <script src="/public/admin-css/js/Chart.min.js"></script>
        <script src="/public/admin-css/js/fileinput.js"></script>
        <script src="/public/admin-css/js/chartData.js"></script>
        <script src="/public/admin-css/js/main.js"></script>


</body>

</html>