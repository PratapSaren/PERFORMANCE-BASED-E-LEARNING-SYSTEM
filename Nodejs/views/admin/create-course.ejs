	<link rel="stylesheet" href="/public/admin-css/css/style.css">

</head>

<body>
	<div class="brand clearfix">
		<a href="#" class="navbar-brand">Edu<b>sys</b></a>
		<span class="menu-btn"><i class="fa fa-bars"></i></span>
		<ul class="ts-profile-nav">
			
			<li class="ts-account">
				<a href="#"><img src="/public/admin-css/img/ts-avatar.jpg" class="ts-avatar hidden-side" alt=""> <%= user.username %> <i class="fa fa-angle-down hidden-side"></i></a>
				<ul>
					<li><a href="/adminprofile">My Account</a></li>
					
					<li><a href="/logout">Logout</a></li>
				</ul>
			</li>
		</ul>
	</div>

	<div class="ts-main-content">
		<nav class="ts-sidebar">
			<ul class="ts-sidebar-menu">
				
				
				<li class="ts-label">Main</li>
				<li><a href="/admin/admin-dashboard"><i class="fa fa-dashboard"></i> Dashboard</a></li>
				<li><a href="#"><i class="fa fa-desktop"></i> Courses </a>
					<ul>
						<li><a href="/admin/create-course"> Create Courses </a></li>
						
						
					</ul>
				</li>
				
				
				

				<!-- Account from above -->
				<ul class="ts-profile-nav">
					
					<li class="ts-account">
						<a href="#"><img src="/public/admin-css/img/ts-avatar.jpg" class="ts-avatar hidden-side" alt=""> <%= user.username %> <i class="fa fa-angle-down hidden-side"></i></a>
						<ul>
							<li><a href="/adminprofile">My Account</a></li>
							
							<li><a href="/logout">Logout</a></li>
						</ul>
					</li>
				</ul>

			</ul>
		</nav>
		<div class="content-wrapper">
			<div class="container-fluid"  >

				<div class="row"  >
					<div class="col-md-12" >
					
						<h2 class="page-title" style="text-align: center;"><strong>Create Course</strong></h2>

						<div class="row" >
							<div class="col-md-8" style="margin: 0 15% ;">
								<div class="panel panel-default">
									<div class="panel-heading">Form fields</div>

									<% if( top.length > 0) { %>
										<div  class="alert alert-warning alert-dismissible" role="alert">
											<button type="button" class="close" data-dismiss="alert">&times;</button>
											<p style="color: black; text-align: center;"><strong><<<   <%= top %> >>>   </strong></p>
										  </div>
										<% } %>


									<div class="panel-body">
										<form class="form-horizontal" action="/admin/create-course" method="POST">
											<div class="form-group">
												<label class="col-sm-3 control-label">Course Name :</label>
												<div class="col-sm-8" >
													<input type="text" class="form-control" name="coursename">
												</div>
											</div>

											<div class="hr-dashed"></div>

											<div class="form-group">
												<label class="col-sm-3 control-label">Course Number :</label>
												<div class="col-sm-8" >
													<input type="text" class="form-control" name="coursenumber">
												</div>
											</div>

											  <div class="hr-dashed"></div>

											  <div class="form-group">
												<label class="col-sm-3 control-label" for="courselevel">Course Level :</label>
												<div class="col-sm-8" >
													<select class="form-control" id="courselevel" name="courselevel">
														<option hidden selected>Select one...</option>
														<option>Beginner</option>
														<option>Intermediate</option>
														<option>Advanced</option>
														
													  </select>
												</div>
											</div>

											<div class="hr-dashed"></div>

											<div class="form-group" >
												<label class="col-sm-3 control-label">Primary Language :</label>
												<div class="col-sm-8">
													<input type="text" class="form-control" name="language">
												</div>
											</div>

											<div class="hr-dashed" style="border: 1px solid black;"></div>	

											<!--  Module-1       -->

											<div class="form-group" >
												<label class="col-sm-3 control-label">Module - 1 Name :</label>
												<div class="col-sm-8">
													<input type="text" class="form-control" name="module_one_name">
												</div>
											</div>


											<div class="hr-dashed"></div>	



											<div class="form-group">
												<label class="col-sm-3 control-label">Module Description :</label>
												<div class="col-sm-8 ">
													<textarea class="form-control" rows="5" name="description_one" ></textarea>
											</div>
											</div>

											<div class="hr-dashed"></div>	


											<div class="form-group">
												<label class="col-sm-3 control-label">Add PDF Link :</label>
												<div class="col-sm-8 ">
													<input type="url" class="form-control" name="pdf_one">
											</div>
											</div>



											<div class="hr-dashed"></div>	



											<div class="form-group">
												<label class="col-sm-3 control-label">Add Video Link :</label>
												<div class="col-sm-8 ">
													<input type="url" class="form-control" name="video_one">
											</div>
											</div>


											<div class="hr-dashed" style="border: 1px solid black;"></div>	
									
											<!-- Module -2 -->


											<div class="form-group" >
												<label class="col-sm-3 control-label">Module - 2 Name :</label>
												<div class="col-sm-8">
													<input type="text" class="form-control" name="module_two_name">
												</div>
											</div>


											<div class="hr-dashed"></div>	



											<div class="form-group">
												<label class="col-sm-3 control-label">Module Description :</label>
												<div class="col-sm-8 ">
													<textarea class="form-control" rows="5" name="description_two" ></textarea>
											</div>
											</div>

											<div class="hr-dashed"></div>	


											<div class="form-group">
												<label class="col-sm-3 control-label">Add PDF Link :</label>
												<div class="col-sm-8 ">
													<input type="url" class="form-control" name="pdf_two">
											</div>
											</div>



											<div class="hr-dashed"></div>	



											<div class="form-group">
												<label class="col-sm-3 control-label">Add Video Link :</label>
												<div class="col-sm-8 ">
													<input type="url" class="form-control" name="video_two">
											</div>
											</div>

											<div class="hr-dashed" style="border: 1px solid black;"></div>	


											<!-- Module - 3 -->


											<div class="form-group" >
												<label class="col-sm-3 control-label">Module - 3 Name :</label>
												<div class="col-sm-8">
													<input type="text" class="form-control" name="module_three_name">
												</div>
											</div>


											<div class="hr-dashed"></div>	



											<div class="form-group">
												<label class="col-sm-3 control-label">Module Description :</label>
												<div class="col-sm-8 ">
													<textarea class="form-control" rows="5" name="description_three" ></textarea>
											</div>
											</div>

											<div class="hr-dashed"></div>	


											<div class="form-group">
												<label class="col-sm-3 control-label">Add PDF Link :</label>
												<div class="col-sm-8 ">
													<input type="url" class="form-control" name="pdf_three">
											</div>
											</div>


											<div class="hr-dashed"></div>	



											<div class="form-group">
												<label class="col-sm-3 control-label">Add Video Link :</label>
												<div class="col-sm-8 ">
													<input type="url" class="form-control" name="video_three">
											</div>
											</div>

											
											<div class="hr-dashed" style="border: 1px solid black;"></div>	


											<div class="form-group">
												<label class="col-sm-3 control-label">M-1 BEGINNER S-1 PDF Link :</label>
												<div class="col-sm-8 ">
													<input type="url" class="form-control" name="m1begs1">
											</div>
											</div>
										
											<div class="hr-dashed"></div>	

											<div class="form-group">
												<label class="col-sm-3 control-label">M-1 BEGINNER S-2 PDF Link :</label>
												<div class="col-sm-8 ">
													<input type="url" class="form-control" name="m1begs2">
											</div>
											</div>
										
											<div class="hr-dashed"></div>	


											<div class="form-group">
												<label class="col-sm-3 control-label">M-1 ADVANCED S-1 PDF Link :</label>
												<div class="col-sm-8 ">
													<input type="url" class="form-control" name="m1advs1">
											</div>
											</div>
										
											<div class="hr-dashed"></div>	

											<div class="form-group">
												<label class="col-sm-3 control-label">M-1 ADVANCED S-2 PDF Link :</label>
												<div class="col-sm-8 ">
													<input type="url" class="form-control" name="m1advs2">
											</div>
											</div>
										
											<div class="hr-dashed"></div>	



											<div class="form-group">
												<label class="col-sm-3 control-label">M-2 BEGINNER S-1 PDF Link :</label>
												<div class="col-sm-8 ">
													<input type="url" class="form-control" name="m2begs1">
											</div>
											</div>
										
											<div class="hr-dashed"></div>	

											<div class="form-group">
												<label class="col-sm-3 control-label">M-2 BEGINNER S-2 PDF Link :</label>
												<div class="col-sm-8 ">
													<input type="url" class="form-control" name="m2begs2">
											</div>
											</div>
										
											<div class="hr-dashed"></div>	


											<div class="form-group">
												<label class="col-sm-3 control-label">M-2 ADVANCED S-1 PDF Link :</label>
												<div class="col-sm-8 ">
													<input type="url" class="form-control" name="m2advs1">
											</div>
											</div>
										
											<div class="hr-dashed"></div>	

											<div class="form-group">
												<label class="col-sm-3 control-label">M-2 ADVANCED S-2 PDF Link :</label>
												<div class="col-sm-8 ">
													<input type="url" class="form-control" name="m2advs2">
											</div>
											</div>
										
											<div class="hr-dashed"></div>	


											<div class="form-group">
												<label class="col-sm-3 control-label">M-3 FINAL S-1 PDF Link :</label>
												<div class="col-sm-8 ">
													<input type="url" class="form-control" name="m3finals1">
											</div>
											</div>
										
											<div class="hr-dashed"></div>	

											<div class="form-group">
												<label class="col-sm-3 control-label">M-3 FINAL S-2 PDF Link :</label>
												<div class="col-sm-8 ">
													<input type="url" class="form-control" name="m3finals2">
											</div>
											</div>
										
											<div class="hr-dashed"></div>	



											<div class="form-group">
												<div  style="text-align: center;">
													<button class="btn btn-primary" type="submit">Submit</button>
												</div>
											</div>

										</form>

									</div>
								</div>
							</div>
							
						</div>
						
				

					</div>
				</div>
				
				
			</div>
		</div>
	</div>

	<footer style="position: fixed;
	left: 0;
	bottom: 0;
	width: 100%;
	background-color: red;
	color: white;
	text-align:center;
	">
