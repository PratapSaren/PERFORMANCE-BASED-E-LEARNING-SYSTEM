	<script type="text/javascript">
		window.onload = function(){ 
			$('#myModal').modal('show');
		 }
	</script>


</head>
	

<body >
	<div class="brand clearfix">
		<a href="#" class="navbar-brand">Edu<b>sys</b></a>
		<span class="menu-btn"><i class="fa fa-bars"></i></span>
		<ul class="ts-profile-nav">

			<li class="ts-account">
				<a href="#"><img src="/public/admin-css/img/ts-avatar.jpg" class="ts-avatar hidden-side" alt="">
					<%= user.username %><i class="fa fa-angle-down hidden-side"></i>
				</a>
				<ul>
					<li><a href="/profile">My Account</a></li>
					
					<li><a href="/logout">Logout</a></li>
				</ul>
			</li>
		</ul>
	</div>

	<div class="ts-main-content" style=" height: 90vh; background-color: rgb(157, 243, 157);">
		<nav class="ts-sidebar">
			<ul class="ts-sidebar-menu">

				<li class="ts-label">Overview</li>
				<li><a href="/student/student-dashboard"><i class="fa fa-pencil" aria-hidden="true"></i> Dashboard</a>
				</li>
				<li><a href="#"><i class="fa fa-pencil" aria-hidden="true"></i> Web Development </a>
					<ul>
						<li><a href="#"><i class="fa fa-futbol-o" aria-hidden="true"></i> MODULE - 1 </a>
						</li>
						<li><a href="#"><i class="fa fa-futbol-o" aria-hidden="true"></i> MODULE - 2 </a></li>
						<li class="open"><a href="#"><i class="fa fa-futbol-o" aria-hidden="true"></i> MODULE - 3 </a></li>

					</ul>
				</li>
				

				<!-- Account from above -->
				<ul class="ts-profile-nav">

					<li class="ts-account">
						<a href="#"><img src="img/ts-avatar.jpg" class="ts-avatar hidden-side" alt="">
							<%= user.username %><i class="fa fa-angle-down hidden-side"></i>
						</a>
						<ul>
							<li><a href="/profile">My Account</a></li>
							
							<li><a href="/logout">Logout</a></li>
						</ul>
					</li>
				</ul>

			</ul>
		</nav>


		<div class="content-wrapper" style="background-color: rgb(157, 243, 157);">
			<div class="container-fluid">

				<div class="row">
					<div class="col-md-12" >

						<% obj.forEach(element =>{ %>
							<% var object = element %> 

						

						<h2 class="page-title"
							style="text-align: center; font-family: Georgia, 'Times New Roman', Times, serif; font-weight: 700;">
							<%= object.coursename %>
						</h2>

						<div class="row" >


							<% if( pass.length > 0) { %>
								<div  class="alert alert-warning alert-dismissible" role="alert">
									<button type="button" class="close" data-dismiss="alert">&times;</button>
									<p style="color: black; text-align: center;"><strong><<<   <%= pass %> >>>   </strong></p>
								  </div>
								<% } %>


							<div class="col-md-12" >
								<div class="row" >


									<div class="col-md-7">
										<div class="panel panel-default"
											style=" margin-top: 20px;">
											<div class="panel-heading">Name Of The Module : <%= object.module_three_name %></div>
											<div class="panel-body" >
												<h1
													style="font-size: medium; font-family: 'Times New Roman', Times, serif;">
													Description :</h1>
												<div id="legendDiv" style="height: auto; ">
													<p
														style="font-family:'Times New Roman', Times, serif; white-space:pre-line; text-align: justify;">
														<%= object.description_three %> 
													</p>

												</div>
											</div>
										</div>
									</div>



									<div class="col-md-5">
										<div class="panel panel-default"
											style=" margin-top: 20px;">
											<div class="panel-heading">VIDEO</div>
											<div class="panel-body">
												<div class="row">
													<div class="col-md-12" >
														<div class="chart chart-doughnut">
															<div class="embed-responsive embed-responsive-16by9">
																<iframe class="embed-responsive-item"
																	src="<%= object.video_three %>"
																	allowfullscreen></iframe>
															</div>

														</div>
													</div>

												</div>
											</div>
										</div>
									</div>



								</div>





							</div>


							<div class="col-md-12" >

								<div class="row" >
									<div class="panel panel-warning">
										<div class="panel-heading">
											<h3 class="panel-title">STUDY MATERIALS </h3>
										</div>
										<div class="panel-body"
											style="border: 2px solid #f47c3c;  font-family:Verdana, Geneva, Tahoma, sans-serif; font-weight: 600; ">

											PDF NOTES FOR MODULE - 3
											<a href="<%= object.pdf_three %>" class="btn btn-warning" style="float: right;">DOWNLOAD</a>
										</div>
									</div>


									<div class="panel panel-danger">
										<div class="panel-heading">
											<h3 class="panel-title">MODULE EVALUATION TEST</h3>
										</div>
										<div class="panel-body"
											style="border: 2px solid #d9534f;  font-family:Verdana, Geneva, Tahoma, sans-serif; font-weight: 600; ">
											MODULE - 3 EVALUATION TEST
											<a href="/test/final" class="btn btn-danger" style="float: right;">TAKE TEST</a>

										</div>
									</div>

									

								</div>


							</div>








						</div>

						<% }) %> 


					</div>




				</div>

			</div>
			<!--hello-->

				<style>
					 .modal-confirm {		
	color: #434e65;
	width: 525px;
}
.modal-confirm .modal-content {
	padding: 20px;
	font-size: 16px;
	border-radius: 5px;
	border: none;
}
.modal-confirm .modal-header {
	background: #47c9a2;
	border-bottom: none;   
	position: relative;
	text-align: center;
	margin: -20px -20px 0;
	border-radius: 5px 5px 0 0;
	padding: 35px;
}
.modal-confirm h4 {
	text-align: center;
	font-size: 36px;
	margin: 10px 0;
}
.modal-confirm .form-control, .modal-confirm .btn {
	min-height: 40px;
	border-radius: 3px; 
}
.modal-confirm .close {
	position: absolute;
	top: 15px;
	right: 15px;
	color: #fff;
	text-shadow: none;
	opacity: 0.5;
}
.modal-confirm .close:hover {
	opacity: 0.8;
}
.modal-confirm .icon-box {
	color: #fff;		
	width: 95px;
	height: 95px;
	display: inline-block;
	border-radius: 50%;
	z-index: 9;
	border: 5px solid #fff;
	padding: 15px;
	text-align: center;
}
.modal-confirm .icon-box i {
	font-size: 64px;
	margin: -4px 0 0 -4px;
}
.modal-confirm.modal-dialog {
	margin-top: 80px;
}
.modal-confirm .btn, .modal-confirm .btn:active {
	color: #fff;
	border-radius: 4px;
	background: #eeb711 !important;
	text-decoration: none;
	transition: all 0.4s;
	line-height: normal;
	border-radius: 30px;
	margin-top: 10px;
	padding: 6px 20px;
	border: none;
}
.modal-confirm .btn:hover, .modal-confirm .btn:focus {
	background: #eda645 !important;
	outline: none;
}
.modal-confirm .btn span {
	margin: 1px 3px 0;
	float: left;
}
.modal-confirm .btn i {
	margin-left: 1px;
	font-size: 20px;
	float: right;
}
				</style>








<div id="myModal" class="modal fade">
	<div class="modal-dialog modal-confirm">
		<div class="modal-content">
			<div class="modal-header justify-content-center">
				<div class="icon-box">
					<i class="fa fa-trophy"></i>
				</div>
				<button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
			</div>
			<div class="modal-body text-center">
				<h4>CONGRATULATIONS!</h4>	
				<p>You Have Completed This Course Thank You For Your Participation In This Course</p>
			
			</div>
		</div>
	</div>
</div> 







			<!---hello-->
		</div>

	</div>




	</div>

